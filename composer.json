{
  "name": "hadder/nfse-coplan",
  "description": "Pacote para geração de NFSe Coplan usando componentes NFePHP (https://github.com/nfephp-org)",
  "keywords": [
    "nfse-coplan",
    "nfse",
    "copan"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "HaDDeR\\NfseCoplan\\": "src/"
    },
    "files": [
      "Helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Fernando Friedrich",
      "email": "fernando@hadder.com.br"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1.8 || ^8.0",
    "nesbot/carbon": "^2",
    "nfephp-org/sped-common": "^5.0",
    "respect/validation": "^1.1",
    "symfony/var-dumper": "5.4.*",
    "justinrainbow/json-schema": "5.*",
    "league/flysystem": "^1.1",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-dom": "*",
    "ext-curl": "*",
    "tecnickcom/tcpdf": "^6.4",
    "ext-simplexml": "*",
    "ext-gd": "*"
  },
  "config": {
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
